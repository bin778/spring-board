<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyb.board.mapper.BoardMapper">

    <insert id="insertBoard" parameterType="com.cyb.board.dto.BoardDto">
        INSERT INTO board (title, description, writer, file_url)
        VALUES (#{title}, #{content}, #{writer}, #{fileUrl})
    </insert>
</mapper>